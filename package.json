		"@babel/preset-env": "^7.10.2",
		"@babel/preset-typescript": "^7.10.1",
		"@types/bent": "^7.0.2",
		"@types/child-process-promise": "^2.2.1",
		"@types/fs-extra": "^9.0.1",
		"@types/inquirer": "^6.5.0",
		"@types/jest": "^26.0.0",
		"@types/lodash": "^4.14.154",
		"@types/node": "^14.0.6",
