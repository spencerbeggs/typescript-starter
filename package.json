{
	"name": "typescript-starter",
	"version": "1.0.0",
	"description": "A bootstrap-able GitHub template to jumpstart Typescript projects.",
	"main": "src/index.ts",
	"author": {
		"name": "C. Spencer Beggs",
		"email": "spencer@beg.gs",
		"url": "https://github.com/spencerbeggs",
		"organization": false
	},
	"repository": {
		"url": "https://github.com/spencerbeggs/typescript-starter.git",
		"type": "git"
	},
	"license": "MIT",
	"scripts": {
		"test": "jest",
		"bootstrap": "ts-node lib/bootstrap.ts init",
		"build": "rm -rf dist && tsc -p build.json && ts-node lib/package",
		"lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx,.json}' --quiet --fix --ignore-path .gitignore",
		"start": "ts-node --project tsconfig.json -r tsconfig-paths/register lib/start.ts"
	},
